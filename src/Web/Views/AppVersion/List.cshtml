@model IEnumerable<Dgg.Cqrs.Sample.Core.Presentation.Models.Admin.AppVersion>
		   
@{
    ViewBag.Title = "All Versions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>All Versions</h2>

<p>
    @Html.ActionLink("Create", "Create")
</p>
<table>
    <tr>
        <th>
            Name
        </th>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        @Html.Partial("_Actions", item)
    </tr>
}

</table>
